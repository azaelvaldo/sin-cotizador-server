    "@hapi/jwt": "^3.2.0",
    "@prisma/client": "^5.18.0",
    "@turf/turf": "^7.2.0",
    "amqplib": "^0.10.8",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "prisma": "^5.18.0",
    "ws": "^8.18.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.3.0",
    "@types/ws": "^8.18.1",
    "prettier": "^3.6.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
